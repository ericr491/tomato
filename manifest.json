{
  "name": "tomato",
  "description": "behold the glorious tomato!",
  "version": "0.2.0",
  "manifest_version": 2,
  "icons": {
    "16": "./icon-16x16.png",
    "32": "./icon-32x32.png",
    "64": "./icon-64x64.png",
    "128": "./icon-128x128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "<all_urls>", "storage"]
}
